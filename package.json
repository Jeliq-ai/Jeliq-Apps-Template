{
  "name": "Jeliq-Apps-Template",
  "version": "0.0.1",
  "jeliq": {
    "template": {
      "repository": "https://github.com/Jeliq-ai/Jeliq-Apps-Template.git",
      "branch": "main",
      "version": "1.4.0"
    }
  },
  "private": true,
  "scripts": {
    "test": "jest",
    "lint:all": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "gen:api:server": "node_modules/@core/bin/gen-api-server.sh",
    "gen:api:cli": "node_modules/@core/bin/gen-api-client.sh",
    "gen:api:docs": "npx -y @redocly/cli build-docs ./src/infra/api/schema/schema.json -o ./src/infra/api/schema/docs.html || true",
    "gen:api": "pnpm gen:api:server && pnpm gen:api:cli",
    "run:storybook": "storybook dev",
    "test:storybook": "test-storybook",
    "build": "pnpm build:api && pnpm build:web",
    "gen:app-router": "node_modules/@router/bin/gen-app-router.js",
    "build:web": "pnpm gen:app-router && next build",
    "dev:web": "next dev",
    "run:web": "next start",
    "build:api": "pnpm gen:api && tsc -p ./tsconfig.api.json && tsc-alias -p ./tsconfig.api.json",
    "ensure-buckets": "node_modules/@backend/bin/ensure-buckets.js",
    "dev:api": "nodemon --config ./api/nodemon.json",
    "run:api": "node dist/api/api/index.js"
  },
  "dependencies": {
    "@backend": "npm:@jeliq/app-sdk-backend-supabase@^3.0.7",
    "@core": "npm:@jeliq/app-sdk-core@^3.0.6",
    "@llm": "npm:@jeliq/app-sdk-llm@^1.2.5",
    "@router": "npm:@jeliq/app-sdk-next-router@^2.0.16",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.27",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dayjs": "^1.11.19",
    "express": "^4.22.1",
    "express-async-errors": "^3.1.1",
    "express-openapi": "^12.1.3",
    "express-openapi-validator": "^5.6.2",
    "next": "^14.2.35",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-native": "^0.73.11",
    "react-native-web": "^0.19.13",
    "react-qr-code": "^2.0.18",
    "react-qr-reader": "3.0.0-beta-1",
    "react-toastify": "^11.0.5",
    "react-webcam": "^7.2.0",
    "recharts": "^3.7.0",
    "tailwindcss": "^3.4.19",
    "uuid": "^10.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@storybook/addon-essentials": "8.3.3",
    "@storybook/addon-links": "8.3.3",
    "@storybook/builder-vite": "8.3.3",
    "@storybook/react": "8.3.3",
    "@storybook/react-vite": "8.3.3",
    "@storybook/test": "8.3.3",
    "@storybook/test-runner": "^0.17.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.4.0",
    "@types/express": "^5.0.6",
    "@types/react": "^18.3.28",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-native-web": "^0.19.13",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.3",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "msw": "^2.12.10",
    "msw-storybook-addon": "^2.0.6",
    "nodemon": "^3.1.14",
    "prettier": "^3.8.1",
    "storybook": "8.3.3",
    "storycap": "^4.3.1",
    "supabase": "^2.76.15",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-native": "^0.73.8"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
