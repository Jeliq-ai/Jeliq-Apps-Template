{
  "name": "Jeliq-App-Template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint:all": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "gen:api:server": "node_modules/@core/bin/gen-api-server.sh",
    "gen:api:cli": "node_modules/@core/bin/gen-api-client.sh",
    "gen:api:docs": "npx -y @redocly/cli build-docs ./src/services/api/schema.json -o ./src/services/api/docs.html || true",
    "gen:api": "pnpm gen:api:server && pnpm gen:api:cli && pnpm gen:api:docs",
    "run:storybook": "storybook dev",
    "test:storybook": "test-storybook",
    "build": "pnpm build:api && pnpm build:web",
    "build:web": "node_modules/@router/bin/gen-app-router.js && next build",
    "dev:web": "next dev",
    "run:web": "next start",
    "build:api": "pnpm gen:api && tsc -p ./tsconfig.api.json",
    "dev:api": "nodemon --config ./api/nodemon.json"
  },
  "dependencies": {
    "@backend": "npm:@jeliq/app-sdk-backend-supabase@^1.0.3",
    "@core": "npm:@jeliq/app-sdk-core@^1.0.7",
    "@router": "npm:@jeliq/app-sdk-next-router@^1.0.7",
    "autoprefixer": "^10.4.20",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-openapi": "^12.1.3",
    "express-openapi-validator": "^5.3.9",
    "next": "^14.2.13",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-native": "^0.73.9",
    "react-native-web": "^0.19.12",
    "tailwindcss": "^3.4.13",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@storybook/addon-essentials": "8.3.3",
    "@storybook/addon-links": "8.3.3",
    "@storybook/builder-vite": "8.3.3",
    "@storybook/react": "8.3.3",
    "@storybook/react-vite": "8.3.3",
    "@storybook/test": "8.3.3",
    "@storybook/test-runner": "^0.17.0",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^3.7.7",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-native-web": "^0.19.12",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "msw": "^2.4.9",
    "msw-storybook-addon": "^2.0.3",
    "nodemon": "^3.1.7",
    "prettier": "^2.8.8",
    "storybook": "8.3.3",
    "storycap": "^4.3.1",
    "supabase": "^1.200.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-native": "^0.73.8"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}